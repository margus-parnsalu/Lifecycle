{% extends "base.jinja2" %}
{% block content %}


    <h3>User</h3>

    <div class="well">

        <form action="" method="post" name="UserForm">

         {% include 'form_error.jinja2' %}

          <div class="form-group">
            {{ form.username.label }}
            {{ form.username() }}
          </div>
          <div class="form-group">
            {{ form.pwd.label }}
            {{ form.pwd() }}
          </div>
          <div class="form-group">
            {{ form.groups.label }}
            {{ form.groups }}
          </div>
          <div class="form-group">
              <button type="submit" class="btn btn-default">Submit</button>
          </div>
        </form>

    </div>

{% endblock %}
